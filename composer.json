{
  "name": "sonusgeneration/fat-free-blocks",
  "license": "GPL-3.0",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "bcosca/fatfree": "3.*",
    "league/flysystem": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Blocks\\": "Blocks/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "Blocks\\Installation\\ComposerScripts::postInstall"
    ]
  }
}
